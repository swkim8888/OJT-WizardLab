CC				= gcc
CPP				= gcc -E
CFLAGS			= -g -O2
DEFS			= -DPACKAGE_NAME=\"like\" -DPACKAGE_TARNAME=\"like\" -DPACKAGE_VERSION=\"0.0.1\" -DPACKAGE_STRING=\"like\ 0.0.1\" -DPACKAGE_BUGREPORT=\"swkim@wizardlab.co.kr\" -DPACKAGE_URL=\"\" -DHAVE_LIBC=1 -DHAVE_LIBM=1 -DSTDC_HEADERS=1 -DHAVE_SYS_TYPES_H=1 -DHAVE_SYS_STAT_H=1 -DHAVE_STDLIB_H=1 -DHAVE_STRING_H=1 -DHAVE_MEMORY_H=1 -DHAVE_STRINGS_H=1 -DHAVE_INTTYPES_H=1 -DHAVE_STDINT_H=1 -DHAVE_UNISTD_H=1 -DHAVE_FCNTL_H=1 -DHAVE_STDLIB_H=1 -DHAVE_STRING_H=1 -DHAVE_SYS_IOCTL_H=1 -DHAVE_TERMIOS_H=1 -DHAVE_UNISTD_H=1 -DHAVE_DIRENT_H=1 -DHAVE_STRCHR=1 -DHAVE_STRDUP=1 -DHAVE_STDLIB_H=1 -DHAVE_MALLOC=1
LIBS			= -lm -lc 

all : like

like : like.c
	$(CC) $(CFLAGS) $(DEFS) $(LIBS) -o $@ $?

clean :
	-rm -rf like
